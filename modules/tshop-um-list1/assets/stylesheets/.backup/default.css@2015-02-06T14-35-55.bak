_line-height: 30px; color: #FFF; font-size: 14px; font-weight: 700; overflow: hidden; }
.tshop-um-list1 .tgbd .info .amico .btn-bd { display: none; }
.tshop-um-list1 .tgbd .info-hidden .info { display: none; }
