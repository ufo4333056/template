.tshop-um-list1 .tghd .more { float: right; margin: -39px 13px 0 0; width: 77px; height: 30px; overflow: hidden; background-position: 0 -1938px; }
.tshop-um-list1 .hdtype2 .more, .tshop-um-list1 .hdtype4 .more { background-position: 0 -1969px; }
.tshop-um-list1 .hdtype .tghd .more, .tshop-um-list1 .hdtype4 .tghd .more { margin-top: -32px; }
.tshop-um-list1 .tgbd-top-bd-no .tgbd, .tshop-um-list1 .hdtype0 .tgbd { margin-top: 0px; }
.tshop-um-list1 .tgbd { margin-top: 6px; overflow: hidden; }
